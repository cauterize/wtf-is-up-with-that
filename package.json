    "ng": "ng",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "start": "ng serve --ssl",
    "test": "ng test",
    "test:ci": "ng test --browsers=CustomHeadless --watch=false",
    "watch": "ng build --watch --configuration development"
  },
