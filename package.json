{
  "name": "backofficev2",
  "version": "0.0.0",
  "scripts": {
    "audit": "npm audit --production --registry=https://registry.npmjs.org --audit-level=low",
    "build": "ng build --configuration=production --base-href=REPLACE_THIS_BASEURL",
    "lint:ci": "eslint ./src --ext .ts,.tsx --config ./.eslintrc.json --ignore-path ./.eslintignore --max-warnings=0",
